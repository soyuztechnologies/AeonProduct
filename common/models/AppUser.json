{
  "name": "AppUser",
  "plural": "AppUsers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "TechnicalId": {
      "type": "string",
      "required": true
    },
    "EmailId": {
      "type": "string",
      "required": true
    },
    "Status": {
      "type": "string",
      "enum": ["Pending", "Reject","Approved"],
      "default": "Pending"
    },
    "Blocked": {
      "type": "string",
      "enum": ["No", "Yes"],
      "default": "No"
    },
    "UserName": {
      "type": "string",
      "required": false
    },
    "Role": {
      "type": "string",
      "required": true
    },
    "JoiningDate": {
      "type": "date"
    },
    "BDay": {
      "type": "date"
    },
    "Salary":{
      "type": "number"
    },
    "LeaveQuota":{
      "type": "number",
      "default": 8
    },
    "Holiday":{
      "type": "string",
      "default": "Monday"
    },
    "MobileNo":{
      "type": "string"
    },
    "CreatedOn": {
      "type": "date"
    },
    "CreatedBy": {
      "type": "string",
      "default": "null"
    },
    "ChangedOn": {
      "type": "date"
    },
    "ChangedBy": {
      "type": "string",
      "default": "null"
    }
  },
  "validations": [],
  "relations":{},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$unauthenticated",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
