<mvc:View
	controllerName="ent.ui.ecommerce.controller.userVerify"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

<MessagePage
	title="{appView>/messagePageText}"
	text="{appView>/messagePageText}"
	description="{appView>/timerText}"
	icon="{appView>/verifyIcon}">
	<buttons>
		<Button text="Logout" press="onMessagePageLogout" visible="{appView>/verifyLogout}"></Button>
	</buttons>
	
</MessagePage>

</mvc:View>