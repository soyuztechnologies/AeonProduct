<mvc:View controllerName="ent.ui.ecommerce.controller.App" displayBlock="false" height="100%"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:fiori="sap.ui.webc.fiori"
    xmlns:tnt="sap.tnt">
    <tnt:ToolPage sideExpanded="{appView>/sideNavExpended}">
        <tnt:header >
            <tnt:ToolHeader  visible="true">
                <!-- <core:Fragment fragmentName="ent.ui.ecommerce.fragments.Toolbar" type="XML" /> -->
               
               
                   <Button visible="{appView>/hamburgerVisibility}"
                    press="onClickMenuButton"
                   icon="sap-icon://menu2">

                   </Button>
                   <Title text="Aeon Products"></Title>
                   <ToolbarSpacer/>
                   <Button press="onPressLogout" visible="{appView>/logoutVisibility}" text="Logout"></Button>
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <!-- <App  id="app" busy="{appView>/busy}"  busyIndicatorDelay="{appView>/delay}" navigate="onNavigation"> -->
                
            <!-- </App> -->
            <!-- <NavContainer > -->
                <f:FlexibleColumnLayout id="layout" layout="{appView>/layout}" backgroundDesign="Translucent" beginColumnNavigate="onNavigation" />
            <!-- </NavContainer> -->
        </tnt:mainContents>
        <tnt:sideContent>
            <tnt:SideNavigation bindingContexts="true"  expanded="{appView>/sideNavExpended}" id="sideNavigation" visible="{appView>/visibility}"  itemSelect="onSelectItem">
                <tnt:NavigationList>
                    <tnt:NavigationListItem text="Upload XML" icon="sap-icon://upload"  key="UploadXml"/>
                    <tnt:NavigationListItem text="All Printers" icon="sap-icon://business-card" key="WelcomePrinters"/>
                    <tnt:NavigationListItem text="Printing Details" icon="sap-icon://print" key="PrintingDetails"/>
                    <tnt:NavigationListItem text="User Details" icon="sap-icon://family-care" key="userDetails"/>
                    <tnt:NavigationListItem text="Profile" icon="sap-icon://customer" key="Profile"/>
                </tnt:NavigationList>
            </tnt:SideNavigation>
        </tnt:sideContent>
    </tnt:ToolPage>
</mvc:View>