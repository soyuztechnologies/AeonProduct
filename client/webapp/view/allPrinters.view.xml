<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="ent.ui.ecommerce.controller.allPrinters" >
  <Page id='idWelcomeP' showHeader="false">
	  <subHeader>
		<Toolbar>
				<content>
					<Title text="Welcome Printer"/>
					<ToolbarSpacer/>
					<SearchField liveChange="onSearchJob" width="auto"/>
				</content>
		</Toolbar>
	  </subHeader>
		<Toolbar>
		   <content>
				 <Title text="Job Status"/>
				 <ToolbarSpacer/>
			     <ComboBox  selectedKey="new_1"  id="jobStatusComboBox"
				            selectionChange="onComboBoxSelectionChange">
							<!-- items="{appView>/jobsData}"
							text="{appView>Status}"> -->
                  <core:Item key="new_1" text="New" />
                  <core:Item key="new_2" text="New" />
                  <core:Item key="new_3" text="New" />
                  <core:Item key="new_4" text="New" />
                  <core:Item key="new_5" text="New" />
                  <core:Item key="new_6" text="New" />
                  <core:Item key="new_7" text="New" />
                </ComboBox>
			</content>
		</Toolbar>
      <List  id="idListAllPrinters" itemPress="onListItemPress" items="{appView>/jobsData}">
			<items>
					<ObjectListItem
							type="Navigation"
							title="{appView>nameOFTheProduct}"
							intro="{appView>jobCardNo}">
							<firstStatus>
							    <ObjectStatus text="{appView>Status}" />
							</firstStatus>
					</ObjectListItem>
			</items>	
	  </List>
  </Page>
</mvc:View>


