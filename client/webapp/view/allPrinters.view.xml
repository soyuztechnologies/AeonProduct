<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="ent.ui.ecommerce.controller.allPrinters" >
  <Page id='idWelcomeP' showHeader="false">
	  <subHeader>
		<Toolbar>
				<content>
					<Title text="Welcome Printer"/>
				</content>
		</Toolbar>
	  </subHeader>
		<Toolbar>
		   <content>
				<SearchField liveChange="onSearchJob" width="50%"/>
				<ToolbarSpacer/>
				<Button icon="sap-icon://filter" press="onFilterPressJobStatus"  tooltip="Status Filter"/>     
			</content>
		</Toolbar>
      <List  id="idListAllPrinters" itemPress="onListItemPress" items="{appView>/jobsData}">
			<items>
					<ObjectListItem
							type="Navigation"
							title="{appView>nameOFTheProduct}"
							intro="{appView>CustomerId/expandTo/appUser}"
							number="{appView>jobCardNo}"
							>
							<firstStatus>
							    <ObjectStatus text="{appView>status}" state="{path: 'appView>status', formatter: '.formatter.getStatusColor'}" />
							</firstStatus>
					</ObjectListItem>
			</items>	
	  </List>
  </Page>
</mvc:View>


