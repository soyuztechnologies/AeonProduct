<core:FragmentDefinition xmlns:form="sap.ui.layout.form"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:i="sap.ui.core"	 xmlns:f = "sap.ui.layout.form"
    xmlns:html="http://www.w3.org/1999/xhtml">
   
  
    <Label text="No of Ups"/>
    <!-- ="{parts:[ {path:'appView>noOfUps-1'}, { path: 'appView>noOfUps-2'}, {path: 'appView>noOfUps-3'} ] ,formatter:'.formatter.sizeFormatter'}" -->
    <Input value="{parts:[ {path:'appView>noOfUps-1'}, { path: 'appView>noOfUps-2'}, {path: 'appView>noOfUps-3'} ] ,formatter:'.formatter.sizeFormatter'}" editable="false"/>

    <Label text="No of Sheets"/>
    <Input value = "{appView>noOfSheets-1}" editable="false"/>

    <Label text="Printing Sheet Size"/>
    <Input value = "{appView>printingSheetSizeL1} x {appView>printingSheetSizeW1}" editable="false"/>

    <Label text="Procured Sheets"/>
    <Input value = "{appView>noOfSheets-2}" editable="false"/>
    
    <Label text="Procured Sheet Size"/>
    <Input value = "{appView>printingSheetSizeL2} x {appView>printingSheetSizeW1}" editable="false"/>

    <Label text="Wastage"/>
    <Input value = "{appView>wastage}" editable="false"/>

    <Label text="Weight in Kgs"/>
    <Input value = "{appView>wtKgs}" editable="false" />
                                
   
</core:FragmentDefinition>