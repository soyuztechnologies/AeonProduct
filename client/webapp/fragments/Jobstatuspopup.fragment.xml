<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
  <ViewSettingsDialog id="myViewSettingsDialog" confirm="onViewSettingsConfirm" cancel="onViewSettingsCancel">
    <filterItems>
      <ViewSettingsFilterItem key="statusFilter" text="Status" multiSelect="true">
        <items>
          <ViewSettingsItem text="New" key="new" />
          <ViewSettingsItem text="In Progress" key="inprogress" />
          <ViewSettingsItem text="Completed" key="completed" />
          <ViewSettingsItem text="Packing" key="packing" />
          <ViewSettingsItem text="In Transit" key="intransit" />
          <ViewSettingsItem text="Delivered" key="delivered" />
          <ViewSettingsItem text="Cancelled" key="cancelled" />
        </items>
      </ViewSettingsFilterItem>
    </filterItems>
  </ViewSettingsDialog>
</core:FragmentDefinition>
