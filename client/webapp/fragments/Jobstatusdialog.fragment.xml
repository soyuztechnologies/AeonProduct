<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:i="sap.ui.core"	 xmlns:f = "sap.ui.layout.form">

<Dialog contentWidth="auto" contentHeight="auto" title="Job Status"
  
resizable="false" >
<content>
    <f:SimpleForm id="jobStatusDialog"
			editable="true"
			layout="ColumnLayout"
            columnsM="2"
			columnsL="3"
			class="sapUiNoMargin sapUiNoPadding"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="JobStatusId" />
				<Text  text="{appView>JobStatusId}" />
				<Label text="CreatedOn" />
				<!-- <Text
					text="{appView>/jobStatusTabData/CreatedOn}"
					
				>
					
				</Text> -->
				<DatePicker dateValue="{appView>CreatedOn}"/>
				<Label text="Raw Material" />
				<Input type="Number" value="{appView>rawMaterial}" />
				<Label text="Coating" />
				<Input type="Number" value="{appView>Coating}" />
				<Label text="Spot UV" />
				<Input type="Number" value="{appView>spotUV}" />
				<Label text="Punching" />
				<Input type="Number" value="{appView>Punching}" />
				<Label text="Packing" />
				<Input type="Number" value="{appView>Packing}" />
				<Label text="Printing" />
				<Input type="Number" value="{appView>Printing}" />
				<Label text="Foiling" />
				<Input type="Number" value="{appView>Foiling}" />
				<Label text="Embossing" />
				<Input type="Number" value="{appView>Embossing}" />
				<Label text="Pasting" />
				<Input type="Number" value="{appView>Pasting}" />
				<Label text="Inc. No." />
				<Input type="Number" value="{appView>InvNo}" />
				<Label text="Delivery No." />
				<Input type="Number" value="{appView>DeliveryNo}" />
				<Label text="Status" />
				<Select id="idStatus" selectedKey="new">
			    	<items>
						<core:Item text="New" key="new" />
						<core:Item text="In Progress" key="inprogress" />
						<core:Item text="Completed" key="completed" />
						<core:Item text="Packing" key="packing" />
						<core:Item text="In Transit" key="intransit" />
						<core:Item text="Delivered" key="delivered" />
						<core:Item text="Cancelled" key="cancelled" />
					</items>
				</Select>

			</f:content>
		</f:SimpleForm>
</content>
<beginButton>
    <Button text="Save" type="Accept" press="onSubmitData" enabled="{appView>/submitEnable}"/>
</beginButton>
<endButton>
    <Button text="Cancel" type="Reject" press="onClose" enabled="{appView>/submitEnable}"/>
</endButton>
</Dialog>	





    
</core:FragmentDefinition>