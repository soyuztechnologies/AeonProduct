<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:i="sap.ui.core"	 xmlns:f = "sap.ui.layout.form">

<Dialog contentWidth="auto" contentHeight="auto" title="{appView>/addJobStatusdialogTitle}"
  
resizable="false" >
<content>
    <f:SimpleForm id="jobStatusDialog"
			editable="true"
			layout="ColumnLayout"
            columnsM="2"
			columnsL="3"
			class="sapUiNoMargin sapUiNoPadding"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="JobStatusId"/>
				<Text  text="{appView>JobStatusId}" />
			
				<Label text="CreatedOn" visible="{appView>/dateFalseforallhead}"/>
				<DatePicker dateValue="{appView>CreatedOn}" visible="{appView>/dateFalseforallhead}"/>

				<Label required="true" text="Raw Material" visible="{appView>/rawMaterialHeadVis}"/>
				<Select forceSelection="false" selectedKey=""  change="onSelectKeyRawMaterial"  id="idRawMaterialPopup" visible="{appView>/rawMaterialHeadVis}" >
					<items>
						<core:Item text="In Stock" key="instock" />
						<core:Item text="Pending" key="pending" />
					</items>
				</Select>
				<Label text="Printing" visible="{appView>/printingHeadVis}"/>
				<Input liveChange="onLiveChange" type="Number" value="{appView>Printing}" visible="{appView>/printingHeadVis}" editable="{appView>/printingHeadEdit}"/>
				<Label text="Coating" visible="{appView>/printingHeadVis}"/>
				<Input type="Number" value="{appView>Coating}" visible="{appView>/printingHeadVis}" editable="{appView>/printingHeadEdit}" />					
				<Label text="Foiling" visible="{appView>/postPressHeadVis}"/>
				<Input type="Number" value="{appView>Foiling}" visible="{appView>/postPressHeadVis}"/>
				<Label text="Spot UV" visible="{appView>/postPressHeadVis}"/>
				<Input type="Number" value="{appView>spotUV}" visible="{appView>/postPressHeadVis}"/>
				<Label text="Embossing" visible="{appView>/postPressHeadVis}"/>
				<Input type="Number" value="{appView>Embossing}" visible="{appView>/postPressHeadVis}"/>

				<Label text="Punching" visible="{appView>/postPressHeadVis}"/>
				<Input type="Number" value="{appView>Punching}" visible="{appView>/postPressHeadVis}"/>
				<Label text="Pasting" visible="{appView>/dispatchHeadVis}"/>
				<Input type="Number" value="{appView>Pasting}" visible="{appView>/dispatchHeadVis}"/>
				<Label text="Packing" visible="{appView>/dispatchHeadVis}" />
				<Input type="Number" value="{appView>/totalShippers}" visible="{appView>/dispatchHeadVis}"  placeholder="No. Of Shippers"/>
				<Input liveChange="onLiveChnagePiecePerBox" type="Number" visible="{appView>/dispatchHeadVis}" editable="{appView>/piecePerBoxEdit}" placeholder="Piece Per Box"/>
				<Input liveChange="onLiveChnagePieceToSend" type="Number" visible="{appView>/dispatchHeadVis}" placeholder="Pieces to be Send"/>


				<Label text="Inv. No." visible="{appView>/accountHeadVis}"/>
				<Input type="Number" value="{appView>InvNo}" visible="{appView>/accountHeadVis}" />
				<Button press="onClickAddStatusAttachment" id="Invstatus" icon="sap-icon://vds-file" type="Emphasized" width="50%" visible="{appView>/accountHeadVis}"/>

				<Label text="Delivery No." visible="{appView>/accountHeadVis}"/>
				<Input type="Number" value="{appView>DeliveryNo}" visible="{appView>/accountHeadVis}"/>
				<Button press="onClickAddStatusAttachment" id="DeliveryStatus" icon="sap-icon://vds-file" type="Emphasized" width="50%" visible="{appView>/accountHeadVis}"/>

				<Label required="true" text="Status" visible="{appView>/jobStatusVis}"/>
				<Select id="idStatus" forceSelection="false" selectedKey="{appView>status}"  change="onSelectKeyStatus" visible="{appView>/jobStatusVis}">
			    	<items>
						<core:Item text="Partially Completed" key="Partially Completed" />
						<!-- <core:Item text="In-Progress" key="inprogress" /> -->
						<core:Item text="Completed" key="Completed" />
						<core:Item text="Packing" key="Packing" />
						<core:Item text="Partially Dispatched" key="Partially Dispatched" />
						<core:Item text="In-Transit" key="In-Transit" />
						<core:Item text="Delivered" key="Delivered" />
						<!-- <core:Item text="Cancelled" key="cancelled" /> -->
					</items>
				</Select>
			</f:content>
		</f:SimpleForm>
</content>
<beginButton>
    <Button text="{appView>/addJobStatusSave}" type="Accept" press="onSubmitData" enabled="{appView>/submitEnable}"/>
</beginButton>
<endButton>
    <Button text="Cancel" type="Reject" press="onClose" enabled="{appView>/submitEnable}"/>
</endButton>
</Dialog>	





    
</core:FragmentDefinition>