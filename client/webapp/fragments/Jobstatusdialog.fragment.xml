<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:t="sap.ui.table" xmlns:i="sap.ui.core"	 xmlns:f = "sap.ui.layout.form">

<Dialog contentWidth="auto" contentHeight="auto" title="{appView>/addJobStatusdialogTitle}"
  
resizable="false" >
<content>
    <f:SimpleForm id="jobStatusDialog"
			editable="true"
			layout="ColumnLayout"
            columnsM="2"
			columnsL="3"
			class="sapUiNoMargin sapUiNoPadding"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="JobStatusId" />
				<Text  text="{appView>JobStatusId}" />
				<Label text="CreatedOn" />
				<!-- <Text
					text="{appView>/jobStatusTabData/CreatedOn}"
					
				>
					
				</Text> -->
				<DatePicker dateValue="{appView>CreatedOn}"/>
				<Label required="true" text="Raw Material" />
				<Select forceSelection="false" selectedKey=""  change="onSelectKeyRawMaterial"  id="idRawMaterialPopup" >
					<items>
						<core:Item text="In Stock" key="instock" />
						<core:Item text="Pending" key="pending" />
					</items>
				</Select>
				<Label text="Coating" />
				<Input type="Number" value="{appView>Coating}" />
				<Label text="Spot UV" />
				<Input type="Number" value="{appView>spotUV}" />
				<Label text="Punching" />
				<Input type="Number" value="{appView>Punching}" />
				<Label text="Packing" />
				<Input type="Number" value="{appView>Packing}" />
				<Label text="Printing" />
				<Input liveChange="onLiveChange" type="Number" value="{appView>Printing}" />
				<Label text="Foiling" />
				<Input type="Number" value="{appView>Foiling}" />
				<Label text="Embossing" />
				<Input type="Number" value="{appView>Embossing}" />
				<Label text="Pasting" />
				<Input type="Number" value="{appView>Pasting}" />

				<Label text="Inv. No." />
				<Input type="Number" value="{appView>InvNo}" />
				<Button press="onClickAddStatusAttachment" id="Invstatus" text="Add" type="Emphasized" />
				

				<Label text="Delivery No." />
				<Input type="Number" value="{appView>DeliveryNo}" />
				<Button press="onClickAddStatusAttachment" id="DeliveryStatus" text="Add" type="Emphasized" />

				<Label required="true" text="Status" />
				<Select id="idStatus" forceSelection="false" selectedKey=""  change="onSelectKeyStatus">
			    	<items>
						<core:Item text="Partially Completed" key="partiallycompleted" />
						<!-- <core:Item text="In-Progress" key="inprogress" /> -->
						<core:Item text="Completed" key="completed" />
						<core:Item text="Packing" key="packing" />
						<core:Item text="Partially Dispatched" key="partiallydispatched" />
						<core:Item text="In-Transit" key="intransit" />
						<core:Item text="Delivered" key="delivered" />
						<!-- <core:Item text="Cancelled" key="cancelled" /> -->
					</items>
				</Select>

			</f:content>
		</f:SimpleForm>
</content>
<beginButton>
    <Button text="{appView>/addJobStatusSave}" type="Accept" press="onSubmitData" enabled="{appView>/submitEnable}"/>
</beginButton>
<endButton>
    <Button text="Cancel" type="Reject" press="onClose" enabled="{appView>/submitEnable}"/>
</endButton>
</Dialog>	





    
</core:FragmentDefinition>