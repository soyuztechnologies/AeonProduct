<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:i="sap.ui.core" xmlns:u="sap.ui.unified"	 xmlns:f = "sap.ui.layout.form" xmlns:html="http://www.w3.org/1999/xhtml">
    <Dialog contentWidth="100%" contentHeight="100%" title="Upload Documents"
  
     id="captchaDialog">
    <content>
    <HBox
    justifyContent="Center"
    alignContent="Center"
    >
        
    <u:FileUploader
    change="onFileUploaddChange"
    buttonText="Browse"
    fileType="docx,jpg,jpeg,xml,xlsx,pdf"
    multiple="false" />
    </HBox>

    <PDFViewer visible="{appView>/pdfVisibility}" source="{path: 'appView>/pdfUrl', formatter:'.formatter.getBase64ToPdf'}"  width="100%" height="600px" />
        <Image visible="{appView>/imgVisibility}"  id="image" src="{appView>/imageContent}" />


    </content>
    <beginButton>
        <Button text="Upload" type="Accept" press="onUploadData" enabled="{appView>/submitEnable}"/>
    </beginButton>
    <endButton>
        <Button text="Cancel" type="Reject" press="onReject" enabled="{appView>/submitEnable}"/>
    </endButton>
</Dialog>		
</core:FragmentDefinition>