<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:i="sap.ui.core"	 xmlns:f = "sap.ui.layout.form"
    xmlns:html="http://www.w3.org/1999/xhtml">
   
        <f:SimpleForm id="AncillaryPartOption"
            editable="true"
            layout="ColumnLayout"
            columnsM="3"
            columnsL="3"
            class="sapUiNoMargin sapUiNoPadding"
            singleContainerFullSize="false" 
            title="Ancillary Part"
            visible="{=${appView>/UserRole} === 'Customer' || ${appView>/UserRole} === 'SalesPerson' ? false : true }">
            <f:content>
                
                <Label	design="Bold" text="Plate" />
                <Text text="{appView>plate}"  />
                <Select visible="{= ${appView>plate} !== null &amp;&amp; ${appView>plate} !== 'None :' }" forceSelection="false" selectedKey="{appView>plateStatus}" change="">
                    <items>
                        <core:Item text="Email Sent" key="EmailSent" enabled="{= ${appView>plate} === 'New :' }"/>
                        <core:Item text="Received" key="Received" enabled="{= ${appView>plate} === 'New :' }"/>
                        <core:Item text="Found" key="Found" enabled="{= ${appView>plate} === 'Old :' }"/>
                    </items>
                </Select>
                            
                <Label text="Pantone Inks" design="Bold"/>
                <Text text="{appView>pantoneInksPart}"/>
                    <Select visible="{= ${appView>pantoneInksPart} !== null &amp;&amp; ${appView>pantoneInksPart} !== 'None :' }" forceSelection="false" selectedKey="{appView>pantoneInkPartStatus}" change="">
                    <items>
                        <core:Item text="Email Sent" key="EmailSent" enabled="{= ${appView>pantoneInksPart} === 'New :' }"/>
                        <core:Item text="Received" key="Received" enabled="{= ${appView>pantoneInksPart} === 'New :' }"/>
                        <core:Item text="Found" key="Found" enabled="{= ${appView>pantoneInksPart} === 'Old :' }"/>
                    </items>
                </Select>
            
                <Label text="Foil Blocks" design="Bold"/>
                <Text text="{appView>foilBlocksPart}"/>
                    <Select visible="{= ${appView>foilBlocksPart} !== null &amp;&amp; ${appView>foilBlocksPart} !== 'None :' }" forceSelection="false" selectedKey="{appView>foilBlocksPartStatus}" change="">
                    <items>
                        <core:Item text="Email Sent" key="EmailSent" enabled="{= ${appView>foilBlocksPart} === 'New :' }"/>
                        <core:Item text="Received" key="Received" enabled="{= ${appView>foilBlocksPart} === 'New :' }"/>
                        <core:Item text="Found" key="Found" enabled="{= ${appView>foilBlocksPart} === 'Old :' }"/>
                    </items>
                </Select>
            
                <Label text="Positive" design="Bold"/>
                <Text text="{appView>positivePart}"/>
                    <Select visible="{= ${appView>positivePart} !== null &amp;&amp; ${appView>positivePart} !== 'None :' }" forceSelection="false" selectedKey="{appView>positivePartStatus}" change="">
                    <items>
                        <core:Item text="Email Sent" key="EmailSent" enabled="{= ${appView>positivePart} === 'New :' }"/>
                        <core:Item text="Received" key="Received" enabled="{= ${appView>positivePart} === 'New :' }"/>
                        <core:Item text="Found" key="Found" enabled="{= ${appView>positivePart} === 'Old :' }"/>
                    </items>
                </Select>
                    
                <Label text="Emboss Block" design="Bold"/>
                <Text text="{appView>embossBlockPart}"/>
                    <Select visible="{= ${appView>embossBlockPart} !== null &amp;&amp; ${appView>embossBlockPart} !== 'None :' }" forceSelection="false" selectedKey="{appView>embossBlockPartStatus}" change="">
                    <items>
                        <core:Item text="Email Sent" key="EmailSent" enabled="{= ${appView>embossBlockPart} === 'New :' }"/>
                        <core:Item text="Received" key="Received" enabled="{= ${appView>embossBlockPart} === 'New :' }"/>
                        <core:Item text="Found" key="Found" enabled="{= ${appView>embossBlockPart} === 'Old :' }"/>
                    </items>
                </Select>
            
                <Label text="Punch" design="Bold"/>
                <Text text="{appView>punchPart}"/>
                    <Select visible="{= ${appView>punchPart} !== null &amp;&amp; ${appView>punchPart} !== 'None :' }" forceSelection="false" selectedKey="{appView>punchPartStatus}" change="">
                    <items>
                        <core:Item text="Email Sent" key="EmailSent" enabled="{= ${appView>punchPart} === 'New :' }"/>
                        <core:Item text="Received" key="Received" enabled="{= ${appView>punchPart} === 'New :' }"/>
                        <core:Item text="Found" key="Found" enabled="{= ${appView>punchPart} === 'Old :' }"/>
                    </items>
                </Select>

            </f:content>
        </f:SimpleForm>


                                
   
</core:FragmentDefinition>