    xmlns="sap.m"
    xmlns:i="sap.ui.core"	 xmlns:f = "sap.ui.layout.form"
    xmlns:html="http://www.w3.org/1999/xhtml">
   
    <Label text="Batch NO." 	design="Bold"/>
    <Text text = "{appView>batchNo}" editable="{appView>/editableFields}"/>

    <Label text="MFG No." 	design="Bold"/>
